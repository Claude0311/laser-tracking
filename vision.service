[Unit]
Description=PosMeas service

[Service]
Type=simple
Environment="DISPLAY=:0.0"
User=pi
ExecStart=/usr/bin/python3 /home/pi/flying-ball-in-hoop/raspi-ballpos/vision.py -p
WorkingDirectory=/home/pi/flying-ball-in-hoop/raspi-ballpos
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=vision

[Install]
WantedBy=multi-user.target
